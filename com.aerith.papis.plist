<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">

<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.aerith.papis</string>

    <key>ProgramArguments</key>
    <array>
      <string>/opt/start_papis.sh</string>
    </array>

    <key>RunAtLoad</key>
    <true />

    <key>KeepAlive</key>
    <true />

    <key>StandardOutPath</key>
    <string>/var/log/papis.out.log</string>

    <key>StandardErrorPath</key>
    <string>/var/log/papis.err.log</string>
  </dict>
</plist>
